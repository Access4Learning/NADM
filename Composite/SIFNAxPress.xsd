	<xs:complexType name="xStudentType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>xStudent Type</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
				<servicePaths>
					<servicePath>
						<path>xLeas/{}/xStudents</path>
						<description>Returns information for all students associated with the LEA specified.</description>
						<xquery>
							<![CDATA[
							declare default element namespace "http://www.sifassociation.org/datamodel/na/3.3";
							/xStudent[enrollment/leaRefId="{}"]
							]]>
						</xquery>
					</servicePath>
					<servicePath>
						<path>xSchools/{}/xStudents</path>
						<description>Returns information for all students based on school enrollment.</description>
						<xquery>
							<![CDATA[
							declare default element namespace "http://www.sifassociation.org/datamodel/na/3.3";
							/xStudent[enrollment/schoolRefId="{}"]
							]]>
						</xquery>
					</servicePath>
					<servicePath>
						<path>xRosters/{}/xStudents</path>
						<description>Returns information for all students present in a roster.</description>
						<xquery>
							<![CDATA[
							declare default element namespace "http://www.sifassociation.org/datamodel/na/3.3";
							for $roster in /xRoster[@refId="{}"]
							for $student in /xStudent
							where $student/@refId = $roster/students/studentReference/refId
							return $student
							]]>
						</xquery>
					</servicePath>
					<servicePath>
						<path>xStudentContacts/{}/xStudents</path>
						<description>Returns information for all students related to a contact.</description>
						<xquery>
							<![CDATA[
							declare default element namespace "http://www.sifassociation.org/datamodel/na/3.3";
							/xStudent[studentContacts/contactPersonRefId="{}"]
							]]>
						</xquery>
					</servicePath>
				</servicePaths>
			</xs:appinfo>
			<xs:documentation></xs:documentation>
		</xs:annotation>
