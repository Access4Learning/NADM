<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:include schemaLocation="../../Composite/SIFNAxPressCommonTypes.xsd" />
    <xs:include schemaLocation="../../Composite/SIFNAxPress.xsd" />
    <xs:include schemaLocation="./SIFNASpecialEducationCommonTypes.xsd" />
    <xs:complexType name="xSpecialEducationAdministrativeDataType">
        <xs:annotation>
            <xs:appinfo>
                <elementName>Special Education</elementName>
                <sifChar>M</sifChar>
                <cedsId/>
                <cedsURL/>
                <definitionRefId>7475bb58-0cae-433c-84ae-780ebb32eb73</definitionRefId>
            </xs:appinfo>
            <xs:documentation>
                Administrative and demographic data supporting Special Education students
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="reevaluationDueDate" type="xSpecialEducationAdministrativeDataReevaluationDueDateType" />
            <xs:element name="parentGuardianList" type="xParentGuardianListType" minOccurs="0" />
            <xs:element name="costSharing" type="xSpecialEducationAdministrativeDataCostSharingType" />
        </xs:sequence>
    </xs:complexType>
    <!-- first level -->
    <xs:complexType name="xSpecialEducationAdministrativeDataReevaluationDueDateType">
        <xs:annotation>
            <xs:appinfo>
                <elementName>
                    Date of next evaluation
                </elementName>
                <sifChar>O</sifChar>
                <cedsId/>
                <cedsURL/>
                <definitionRefId>61015b82-155f-4db9-8ecc-e845d62883fe</definitionRefId>
            </xs:appinfo>
            <xs:documentation>The date that the next evaluation is due based on the three year cycle.</xs:documentation>
        </xs:annotation>
    </xs:complexType>
    <xs:complexType name="xSpecialEducationAdministrativeDataCostSharingType">
        <xs:sequence>
            <xs:element name="status" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <elementName>
                            Cost Sharing Availability Status
                        </elementName>
                        <sifChar>O</sifChar>
                        <cedsId/>
                        <cedsURL/>
                        <definitionRefId>5201310e-c36c-42b1-9fa0-6be95502cd96</definitionRefId>
                    </xs:appinfo>
                    <xs:documentation>Plan has cost sharing available.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="agency" type="xs:normalizedString" minOccurs="0">
                <xs:annotation>
                    <xs:appinfo>
                        <elementName>
                            Name of Agency Contributing Funding
                        </elementName>
                        <sifChar>O</sifChar>
                        <cedsId/>
                        <cedsURL/>
                        <definitionRefId>00be92a9-0521-472f-971d-547a274a3cfa</definitionRefId>
                    </xs:appinfo>
                    <xs:documentation>
                        Government agency contributing funds to the support of this student.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
</xs:schema>