<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:include schemaLocation="../../Composite/SIFNAxPressCommonTypes.xsd"/>
    <xs:include schemaLocation="../../Composite/SIFNAxPress.xsd"/>


    <xs:complexType name="xSpecialEducationIepHistoryType">
        <xs:annotation>
            <xs:appinfo>
                <elementName>IEP History</elementName>
                <events>no</events>
                <isSIFObject>yes</isSIFObject>
                <cedsId/>
                <cedsURL/>
            </xs:appinfo>
            <xs:documentation>
                List of previous IEPs and contact information for additional research.
            </xs:documentation>
        </xs:annotation>


        <xs:complexContent>

            <xs:extension base="gOtherSIFType">

                <xs:sequence>
                    
                    <xs:element name="iepHistoryReference" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:appinfo>
                                <elementName>
                                    IEP History Reference
                                </elementName>
                                <sifChar>O</sifChar>
                                <cedsId/>
                                <cedsURL/>
                            </xs:appinfo>
                            <xs:documentation>
                                Documentation of existence of previous IEP and information about where to find out more information
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                
                                <xs:element name="lea" type="xLeaType" minOccurs="0">
                                    <xs:annotation>
                                        <xs:appinfo>
                                            <elementName>
                                                LEA
                                            </elementName>
                                            <sifChar>O</sifChar>
                                            <cedsId/>
                                            <cedsURL/>
                                        </xs:appinfo>
                                        <xs:documentation>
                                            School where the IEP was created
                                        </xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="iepDates">
                                    <xs:annotation>
                                        <xs:appinfo>
                                            <elementName>
                                                IEP Dates
                                            </elementName>
                                            <sifChar>O</sifChar>
                                            <cedsId/>
                                            <cedsURL/>
                                        </xs:appinfo>
                                        <xs:documentation>
                                            Period that IEP is in effect
                                        </xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                        <xs:sequence>
                                            
                                            <xs:element name="start" type="xs:date" minOccurs="0"/>
                                            <xs:element name="end" type="xs:date" minOccurs="0"/>
                                            
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="contactInfo" type="gContactPersonType" minOccurs="0">
                                    <xs:annotation>
                                        <xs:appinfo>
                                            <elementName>
                                                Contact Info
                                            </elementName>
                                            <sifChar>O</sifChar>
                                            <cedsId/>
                                            <cedsURL/>
                                        </xs:appinfo>
                                        <xs:documentation>
                                            Contact information for additional information about this IEP
                                        </xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    



                </xs:sequence>
            </xs:extension>

        </xs:complexContent>




    </xs:complexType>

</xs:schema>
