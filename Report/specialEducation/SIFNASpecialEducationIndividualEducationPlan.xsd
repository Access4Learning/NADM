										<xs:appinfo>
											<elementName>
												End
											</elementName>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>
											Effective end date of the IEP
										</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="enrollmentGradeLevel" minOccurs="1" maxOccurs="unbounded">
