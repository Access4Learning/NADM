																<xs:appinfo>
																	<elementName>
																		Desired Outcomes List
																	</elementName>
																	<sifChar>O</sifChar>
																	<cedsId/>
																	<cedsURL/>
																</xs:appinfo>
																<xs:documentation>
																	List of specific life achievements desired by student
																</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="desiredOutcome">
																		<xs:annotation>
																			<xs:appinfo>
																				<elementName>
																					Desired Outcome
																				</elementName>
																				<sifChar>O</sifChar>
																				<cedsId/>
																				<cedsURL/>
																			</xs:appinfo>
																			<xs:documentation>
																				A specific achievement the student wants to achieve
																			</xs:documentation>
																		</xs:annotation>
																		<xs:complexType>
																			<xs:sequence>
																				<xs:element name="category" minOccurs="0">
																					<xs:annotation>
																						<xs:appinfo>
																							<elementName>
																								Category
																							</elementName>
																							<sifChar>O</sifChar>
																							<cedsId/>
																							<cedsURL/>
																						</xs:appinfo>
																						<xs:documentation>
																							Category of achievement
																						</xs:documentation>
																					</xs:annotation>
																					<xs:simpleType>
																						<xs:restriction base="xs:normalizedString">
																							<xs:enumeration value="AdultLiving" />
																							<xs:enumeration value="Post-secondary" />
																							<xs:enumeration value="WorkingEnvironment" />
																							<!-- is this list correct? tqii -->
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:element>
																				<xs:element name="title" type="xs:normalizedString" minOccurs="0">
																					<xs:annotation>
																						<xs:appinfo>
																							<elementName>
																								Title
																							</elementName>
																							<sifChar>O</sifChar>
																							<cedsId/>
																							<cedsURL/>
																						</xs:appinfo>
																						<xs:documentation>
																							Brief characterization of achievement
																						</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																				<xs:element name="description" type="xs:normalizedString" minOccurs="0">
																					<xs:annotation>
																						<xs:appinfo>
																							<elementName>
																								Description
																							</elementName>
																							<sifChar>O</sifChar>
																							<cedsId/>
																							<cedsURL/>
																						</xs:appinfo>
																						<xs:documentation>
																							Specific details that would be recognized as successful achievement for this intention
																						</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																				<xs:element name="timeframe" type="xs:normalizedString" minOccurs="0">
																					<xs:annotation>
																						<xs:appinfo>
																							<elementName>
																								Time Frame
																							</elementName>
																							<sifChar>O</sifChar>
																							<cedsId/>
																							<cedsURL/>
																						</xs:appinfo>
																						<xs:documentation>
																							Explanation of when this achievement should take place
																						</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																			</xs:sequence>
																		</xs:complexType>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="academic" minOccurs="1" maxOccurs="unbounded">
