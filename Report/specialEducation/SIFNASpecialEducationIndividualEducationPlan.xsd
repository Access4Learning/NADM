							<xs:appinfo>
								<elementName>
									Effective Period
								</elementName>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>
								Dates specifying start and end of the plan's implementation period
							</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="start" type="xs:date" minOccurs="0">
									<xs:annotation>
										<xs:appinfo>
											<elementName>
												Start
											</elementName>
											<sifChar>O</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation>
											Effective beginning of the IEP
										</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="end" type="xs:date" minOccurs="0">
