																<xs:appinfo>
																	<elementName>
																		Parent's Concern
																	</elementName>
																	<sifChar>O</sifChar>
																	<cedsId/>
																	<cedsURL/>
																</xs:appinfo>
																<xs:documentation>
																	Parent’s explanation of their interest in student’s participation in special education
																</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="studentConcern" type="xs:normalizedString" minOccurs="0">
															<xs:annotation>
																<xs:appinfo>
																	<elementName>
																		Student's Concern
																	</elementName>
																	<sifChar>O</sifChar>
																	<cedsId/>
																	<cedsURL/>
																</xs:appinfo>
																<xs:documentation>
																	Student’s explanation of his or her interest in participation in special education
																</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="strengths" type="xs:normalizedString" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<elementName>
															Strengths
														</elementName>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>
														Explanation of perceived strengths and abilities of the student
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="motivation" type="xs:normalizedString" minOccurs="0">
												<xs:annotation>
													<xs:appinfo>
														<elementName>
															Motivations
														</elementName>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>
														Explanation of the goals and interests of the student
													</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="evaluation" minOccurs="1" maxOccurs="unbounded">
												<xs:annotation>
													<xs:appinfo>
														<elementName>
															Evaluation
														</elementName>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>
														Anecdotal background information about performance
													</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="needsList" minOccurs="1" maxOccurs="unbounded">
															<xs:annotation>
																<xs:appinfo>
																	<elementName>
																		List of Needs
																	</elementName>
																	<sifChar>O</sifChar>
																	<cedsId/>
																	<cedsURL/>
																</xs:appinfo>
																<xs:documentation>
																	Non-technical list of student’s perceived educational special needs
																</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="need" type="xs:normalizedString" minOccurs="0">
																		<xs:annotation>
																			<xs:appinfo>
																				<elementName>
																					Need
																				</elementName>
																				<sifChar>O</sifChar>
																				<cedsId/>
																				<cedsURL/>
																			</xs:appinfo>
																			<xs:documentation>
																				Perceived disability
																			</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
														<xs:element name="performance" minOccurs="1" maxOccurs="unbounded">
															<xs:annotation>
																<xs:appinfo>
																	<elementName>
																		Performance
																	</elementName>
																	<sifChar>O</sifChar>
																	<cedsId/>
																	<cedsURL/>
																</xs:appinfo>
																<xs:documentation>
																	Non-technical list of student’s performance shortcomings
																</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="generalEducation" type="xs:normalizedString" minOccurs="0">
																		<xs:annotation>
																			<xs:appinfo>
																				<elementName>
																					General Education
																				</elementName>
																				<sifChar>O</sifChar>
																				<cedsId/>
																				<cedsURL/>
																			</xs:appinfo>
																			<xs:documentation>
																				Student’s ability to perform classroom and other educational tasks
																			</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="testResultList" minOccurs="1" maxOccurs="unbounded">
																		<xs:annotation>
																			<xs:appinfo>
																				<elementName>
																					Test Result List
																				</elementName>
																				<sifChar>O</sifChar>
																				<cedsId/>
																				<cedsURL/>
																			</xs:appinfo>
																			<xs:documentation>
																				Anecdotal list of previous testing and evaluation of student’s performance
																			</xs:documentation>
																		</xs:annotation>
																		<xs:complexType>
																			<xs:sequence>
																				<xs:element name="testResult" type="xs:normalizedString" minOccurs="0">
																					<xs:annotation>
																						<xs:appinfo>
																							<elementName>
																								Test Result
																							</elementName>
																							<sifChar>O</sifChar>
																							<cedsId/>
																							<cedsURL/>
																						</xs:appinfo>
																						<xs:documentation>
																							Explanation of the result of a test
																						</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																			</xs:sequence>
																		</xs:complexType>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
														<xs:element name="personalGoalsList" minOccurs="1" maxOccurs="unbounded">
															<xs:annotation>
																<xs:appinfo>
																	<elementName>
																		Personal Goal List
																	</elementName>
																	<sifChar>O</sifChar>
																	<cedsId/>
																	<cedsURL/>
																</xs:appinfo>
																<xs:documentation>
																	List of goals that student wants to achieve
																</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="personalGoal">
																		<xs:annotation>
																			<xs:appinfo>
																				<elementName>
																					Personal Goal
																				</elementName>
																				<sifChar>O</sifChar>
																				<cedsId/>
																				<cedsURL/>
																			</xs:appinfo>
																			<xs:documentation>
																				Specific goal
																			</xs:documentation>
																		</xs:annotation>
																		<xs:complexType>
																			<xs:sequence>
																				<xs:element name="description" type="xs:normalizedString" minOccurs="0">
																					<xs:annotation>
																						<xs:appinfo>
																							<elementName>
																								Description
																							</elementName>
																							<sifChar>O</sifChar>
																							<cedsId/>
																							<cedsURL/>
																						</xs:appinfo>
																						<xs:documentation>
																							Explanation of the reason and nature of the goal
																						</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																				<xs:element name="achievementLevel" type="xs:normalizedString" minOccurs="0">
																					<xs:annotation>
																						<xs:appinfo>
																							<elementName>
																								Achievement Level
																							</elementName>
																							<sifChar>O</sifChar>
																							<cedsId/>
																							<cedsURL/>
																						</xs:appinfo>
																						<xs:documentation>
																							What success looks like for this goal
																						</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																			</xs:sequence>
																		</xs:complexType>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="intention" minOccurs="1" maxOccurs="unbounded">
												<xs:annotation>
													<xs:appinfo>
														<elementName>
															Intention
														</elementName>
														<sifChar>O</sifChar>
														<cedsId/>
														<cedsURL/>
													</xs:appinfo>
													<xs:documentation>
														The student's own intentions and aspirations
													</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="overview" type="xs:normalizedString" minOccurs="0">
															<xs:annotation>
																<xs:appinfo>
																	<elementName>
																		Overview
																	</elementName>
																	<sifChar>O</sifChar>
																	<cedsId/>
																	<cedsURL/>
																</xs:appinfo>
																<xs:documentation>
																	Explanation of what the student wants to accomplish in life
																</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="desiredOutcomeList" minOccurs="1" maxOccurs="unbounded">
