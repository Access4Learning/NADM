							<xs:appinfo>
								<elementName>
									Enrollment Grade Level
								</elementName>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>
								The student’s actual, age and experience appropriate grade level including advancement
							</xs:documentation>
						</xs:annotation>
