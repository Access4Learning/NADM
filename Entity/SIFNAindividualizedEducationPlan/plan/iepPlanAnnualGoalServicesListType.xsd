					<xs:appinfo>
						<elementName>
							Inclusive Setting Indicator
						</elementName>
						<sifChar>O</sifChar>
						<cedsId>001634</cedsId>
						<cedsURL>https://ceds.ed.gov/element/001634</cedsURL>
